# Production Environment Variables
REACT_APP_ENVIRONMENT=production

# Wompi Payment Gateway (Production)
# Replace with your actual production keys
REACT_APP_WOMPI_PUBLIC_KEY=pub_prod_your_public_key_here
REACT_APP_WOMPI_PRIVATE_KEY=prv_prod_your_private_key_here
REACT_APP_WOMPI_INTEGRITY_SECRET=your_integrity_secret_here

# Google Analytics
REACT_APP_GA_TRACKING_ID=G-YOUR_TRACKING_ID

# EmailJS Configuration
REACT_APP_EMAILJS_SERVICE_ID=your_service_id
